doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="/styles.css")
        title Document
    body 
        ol 
            each student in results
                a(href=`/students/${student.entityId}/`)
                    li= `${student.entityFields.firstName._value} ${student.entityFields.lastName._value} - ${student.entityFields.points._value} Points`